{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|webp|gif)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "kanban-freshness-tasks",
      "urls": [
        "/api/mobile/tasks"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "kanban-performance-config",
      "urls": [
        "/api/mobile/config"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 10,
        "maxAge": "7d"
      }
    }
  ]
}